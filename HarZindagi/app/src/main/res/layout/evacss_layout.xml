<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="30dp"
    tools:context="com.ipal.itu.harzindagi.Activities.Act"

    >

<ImageView
    android:layout_width="75dp"
    android:layout_height="75dp"
    android:id="@+id/evacs_checkin"
    android:src="@drawable/evacs_checkin"

    />
    <ImageView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/evacs_Kidstation"
        android:layout_marginLeft="15dp"
        android:src="@drawable/evacs_kidstation"
        android:layout_toRightOf="@id/evacs_checkin"


        />
    <ImageView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/evacs_Epi"
        android:src="@drawable/evacs_epi"
        android:layout_marginLeft="15dp"
        android:layout_toRightOf="@id/evacs_Kidstation"


        />
    <ImageView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/evacs_non_Epi"
        android:src="@drawable/evacs_non_epi"
        android:layout_marginTop="15dp"
        android:layout_below="@id/evacs_checkin"
        />
</RelativeLayout>
